{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "apimServiceName": {
      "type": "string"
    }
  },
  "resources": [
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/namedValues",
      "name": "[concat(parameters('apimServiceName'), '/myAPISecret')]",
      "originalName": "myAPISecret",
      "properties": {
        "tags": [],
        "secret": true,
        "displayName": "api-secret",
        "keyVault": {
          "secretIdentifier": "https://kvapimdevops.vault.azure.net/secrets/mySeccret"
        }
      }
    },
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/namedValues",
      "name": "[concat(parameters('apimServiceName'), '/func-authdemo-key')]",
      "originalName": "func-authdemo-key",
      "properties": {
        "tags": [
          "key",
          "function",
          "auto"
        ],
        "secret": true,
        "displayName": "func-authdemo-key"
      }
    },
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/namedValues",
      "name": "[concat(parameters('apimServiceName'), '/Test')]",
      "originalName": "Test",
      "properties": {
        "tags": [],
        "secret": false,
        "displayName": "Test",
        "value": "Test"
      }
    },
    {
      "apiVersion": "2021-08-01",
      "type": "Microsoft.ApiManagement/service/namedValues",
      "name": "[concat(parameters('apimServiceName'), '/TimeNow')]",
      "originalName": "TimeNow",
      "properties": {
        "tags": [],
        "secret": false,
        "displayName": "TimeNow",
        "value": "@(DateTime.Now.ToString())"
      }
    }
  ]
}